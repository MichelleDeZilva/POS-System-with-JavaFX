<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="frmCus" prefHeight="400.0" prefWidth="600.0" style="-fx-background-image: url('WUSL/images/managecus.png');" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WUSL.controller.manageCusController">
   <children>
      <Button layoutX="27.0" layoutY="28.0" mnemonicParsing="false" onAction="#cNewCus" style="-fx-background-color: #C15B78;" text="New Customer">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Label layoutX="27.0" layoutY="76.0" text="ID">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label layoutX="27.0" layoutY="121.0" text="Name">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <Label layoutX="27.0" layoutY="167.0" text="Address">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <TextField fx:id="cusId" layoutX="75.0" layoutY="72.0" prefHeight="25.0" prefWidth="394.0" />
      <TextField fx:id="cusName" layoutX="75.0" layoutY="117.0" prefHeight="25.0" prefWidth="394.0" />
      <TextField fx:id="cusAddress" layoutX="75.0" layoutY="163.0" prefHeight="25.0" prefWidth="394.0" />
      <TableView fx:id="cTable" layoutX="16.0" layoutY="236.0" prefHeight="143.0" prefWidth="560.0">
        <columns>
          <TableColumn prefWidth="193.0" text="ID" />
          <TableColumn prefWidth="188.0" text="Name" />
            <TableColumn prefWidth="178.0" text="Address" />
        </columns>
      </TableView>
      <Button layoutX="93.0" layoutY="200.0" mnemonicParsing="false" onAction="#cSave" prefHeight="25.0" prefWidth="59.0" style="-fx-background-color: #C15B78;" text="Save">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button layoutX="243.0" layoutY="200.0" mnemonicParsing="false" onAction="#cDelete" prefHeight="25.0" prefWidth="59.0" style="-fx-background-color: #C15B78;" text="Delete">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button layoutX="516.0" layoutY="28.0" mnemonicParsing="false" onAction="#cHome" style="-fx-background-color: #189AB4;" text="Home">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button layoutX="388.0" layoutY="200.0" mnemonicParsing="false" onAction="#GenCusReport" prefHeight="25.0" prefWidth="71.0" style="-fx-background-color: #C15B78;" text="Report">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
   </children>
